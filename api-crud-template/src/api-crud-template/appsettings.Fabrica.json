{
  "AppSettings": {
    "DB": {
      "Cluster": "${SPA_CLUSTER_SERVER}", //informar endereço do Clust05  - FAVOR PREENCHER
      "Username": "${SPA_USER}", //informar user spa para banco de dados spabanpara - USO INTERNO, NÃO MUDAR
      "Password": "${SPA_CRIPT_PASSWORD}", //informar password spa para banco de dados spabanpara, criptografado e será decript em tempo de execução - USO INTERNO, NÃO MUDAR
      "Database": "${SPA_DB}", //informar Banco de dados do SPA - USO INTERNO, NÃO MUDAR
      "CommandTimeout": 10, // tempo MAXIMO de espera de execução - USO INTERNO, NÃO MUDAR
      "ConnectTimeout": 10, // tempo MAXIMO de espera conexão - USO INTERNO, NÃO MUDAR
      "ConnectionPool": 0, // não utilizado mais
      "IsTraceExecActive": true //ativa trace execução Sp   - USO INTERNO, NÃO MUDAR
    },
    "Otlp": {
      "ServiceName": "", // não utilizado por hora 
      "ServiceVersion": "", // não utilizado por hora
      "Endpoint": "${OPEN_TELEMETRY_ENDPOINT}" // endereço do opentelemetry  - FAVOR PREENCHER
    },
    "SPA": {
      "Dependencia": {
        "Agencia": 0, // Agencia origem FEP - USO INTERNO, NÃO MUDAR
        "Posto": 1 // Posto origem FEP - USO INTERNO, NÃO MUDAR
      },
      "RouterIP": "${SPAROUTER_IP}", // IP do SPARouter em PRODUÇÃO  - FAVOR PREENCHER
      "RouterPort": 30001, //Porta SPARouter em PRODUÇÃO - FAVOR PREENCHER
      "Operador": "116", // Codigo do Operador SPA - USO INTERNO, NÃO MUDAR
      "ConnectTimeOut": 10000, // Tempo máximo espera na conexão TCP - USO INTERNO, NÃO MUDAR
      "SendTimeout": 30000, // Tempo maximo espera no envio - USO INTERNO, NÃO MUDAR
      "ReceiveBufferSize": 1024 // Tamanho do buffer - USO INTERNO, NÃO MUDAR
    },
    "GC": {
      "Ativo": true, // Ativar a coleta de lixo - USO INTERNO, NÃO MUDAR
      "TimerColetar":30000, // Timer para realização da coleta - USO INTERNO, NÃO MUDAR
      "MemoryTrigger": 300, // Gatilho de memória para execução da coleta - USO INTERNO, NÃO MUDAR
      "AddHoursToUTC": -3 //Ajuste timezone no ambiente - USO INTERNO, NÃO MUDAR    }
    }
  }
}
